
# ----------------------------------------------------------------------------------------
# AWS Configuration
# ----------------------------------------------------------------------------------------

# The AWS region where all resources will be created.
# aws_region = "us-east-1"

# ----------------------------------------------------------------------------------------
# Project Configuration
# ----------------------------------------------------------------------------------------

# A name for the project, used to prefix resource names.
# project_name = "sara"

# The deployment environment (e.g., 'dev', 'staging', 'prod').
# environment = "dev"

# ----------------------------------------------------------------------------------------
# Application Configuration
# ----------------------------------------------------------------------------------------

# The full URL of the backend API.
# api_endpoint_url = "https://api.example.com"

# ----------------------------------------------------------------------------------------
# GitHub Configuration
# ----------------------------------------------------------------------------------------

# The GitHub username or organization that owns the repository.
# github_owner = "your-github-username"

# The name of the GitHub repository.
# github_repo = "sara-front"

# The branch to be used by CodePipeline.
# github_branch = "main"

# The ARN of the AWS CodeStar connection to GitHub.
# This must be created manually in the AWS console under "Developer Tools" > "CodeStar" > "Connections".
# After creating the connection, you will get an ARN to paste here.
# codestar_connection_arn = "arn:aws:codestar-connections:us-east-1:123456789012:connection/a1b2c3d4-5678-90ab-cdef-EXAMPLE12345"
